{
  "name": "es6-module-skeleton",
  "version": "0.0.1",
  "description": "es6-module-skeleton",
  "repository": {
    "type": "git",
    "url": "git@github.com:sugarshin/es6-module-skeleton.git"
  },
  "main": "index.js",
  "files": [
    "lib",
    "index.js",
    "test",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src/",
    "test": "mocha --compilers js:espower-babel/guess test/**/*.js",
    "compile": "babel -d lib/ src/",
    "standalone": "browserify src/es6-module-skeleton.js -t babelify -s ModuleSkeleton | derequire > build/es6-module-skeleton.js",
    "minify": "uglifyjs build/es6-module-skeleton.js -m -c warnings=false --comments '/\\/*!/' -o build/es6-module-skeleton.min.js",
    "build": "npm ru lint && npm ru compile && npm ru standalone && npm ru minify"
  },
  "engines": {
    "node": "0.12.x"
  },
  "author": "sugarshin",
  "license": "MIT"
}
